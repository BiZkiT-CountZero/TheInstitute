package com.gmail.bizkit245;

public class Student extends Human {
	private long recordBook;
	private String group;

	public Student(String name, String lastname, int age, boolean sex, long recordBook, String group) {
		super(name, lastname, age, sex);
		this.recordBook = recordBook;
		this.group = group;
	}

	public Student() {
		super();
	}

	public long getRecordBook() {
		return recordBook;
	}

	public void setRecordBook(long recordBook) {
		this.recordBook = recordBook;
	}

	public String getGroup() {
		return group;
	}

	public void setGroup(String group) {
		this.group = group;
	}

	@Override
	public String toString() {
		return super.toString() + " Student [recordBook=" + recordBook + ", group=" + group + "]";
	}

}
